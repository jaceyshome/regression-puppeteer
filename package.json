{
  "name": "regression-puppeteer",
  "version": "1.0.0",
  "author": "Jake Wang",
  "license": "MIT",
  "description": "Visual regression test, regression test, puppeteer visual test",
  "license": "MIT",
  "keywords": [
    "regression",
    "cucumber",
    "puppeteer",
    "headless",
    "automation",
    "visual test",
    "screenshot"
  ],
  "engines": {
    "node": "9.3.0"
  },
  "config": {
    "imageRepo": "jacobwang05/regression-puppeteer",
    "imageName": "regression-puppeteer",
    "imagePort": "7080"
  },
  "scripts": {
    "server": "http-server ./public -p 7080 --cors true",
    "change-log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build": "node ./scripts/build-features.js",
    "pre-run": "npm run-script build-features",
    "cuke": "cucumber-js --tags 'not @prodOnly'",
    "cuke-watch": "NODE_TEST_MODE=watch cucumber-js --tags '@watch'",
    "run-visual": "cucumber-js --tags '@regressionVisual'",
    "localdev": "NODE_ENV=local-development npm run-script cuke",
    "localdev-watch": "NODE_ENV=local-development npm run-script cuke-watch",
    "dev": "NODE_ENV=development npm run-script cuke",
    "dev-watch": "NODE_ENV=development npm run-script cuke-watch",
    "prod": "NODE_ENV=production cucumber-js --tags '@prod or @prodOnly'",
    "prod-watch": "NODE_ENV=production npm run-script cuke-watch",
    "release": "standard-version",
    "release-patch": "npm run release -- --release-as patch",
    "release-minor": "npm run release -- --release-as minor",
    "predocker:build": "npm run build",
    "docker:build": "cross-conf-env docker image build . -t $npm_package_config_imageRepo:$npm_package_version",
    "postdocker:build": "npm run docker:tag",
    "docker:tag": " cross-conf-env docker image tag $npm_package_config_imageRepo:$npm_package_version $npm_package_config_imageRepo:latest",
    "docker:run": "run-s -c docker:clean docker:runHelper",
    "docker:runHelper": "cross-conf-env docker run -e NODE_ENV=local --name $npm_package_config_imageName -d -p $npm_package_config_imagePort:7080 $npm_package_config_imageRepo",
    "predocker:publish": "echo Attention! Ensure `docker login` is correct.",
    "docker:publish": "cross-conf-env docker image push $npm_package_config_imageRepo:$npm_package_version",
    "postdocker:publish": "cross-conf-env docker image push $npm_package_config_imageRepo:latest",
    "docker:clean": "cross-conf-env docker rm -f $npm_package_config_imageName",
    "predocker:taillogs": "echo Web Server Logs:",
    "docker:taillogs": "cross-conf-env docker logs -f $npm_package_config_imageName",
    "docker:open:win": "echo Trying to launch on Windows && timeout 2 && start http://localhost:%npm_package_config_imagePort%",
    "docker:open:mac": "echo Trying to launch on MacOS && sleep 2 && URL=http://localhost:$npm_package_config_imagePort && open $URL",
    "docker:debugmessage": "echo Docker Debug Completed Successfully! Hit Ctrl+C to terminate log tailing.",
    "predocker:debug": "run-s docker:build docker:run",
    "docker:debug": "run-s -cs docker:open:win docker:open:mac docker:debugmessage docker:taillogs"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "chai": "^4.2.0",
    "cucumber": "^5.0.2",
    "cucumber-html-reporter": "^4.0.4",
    "debug": "^4.1.0",
    "factory_girl": "^0.2.3",
    "fs-copy-file": "^2.1.2",
    "fs-extra": "^7.0.0",
    "http-server": "^0.11.1",
    "lodash": "^4.17.11",
    "lorem-ipsum": "^1.0.6",
    "moment": "^2.22.2",
    "node-resemble-js": "^0.2.0",
    "numeral": "^2.0.6",
    "platform": "^1.3.5",
    "puppeteer": "^1.9.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "require-dir": "^1.1.0"
  },
  "devDependencies": {
    "conventional-changelog": "^3.0.5",
    "conventional-changelog-cli": "^2.0.5",
    "cross-conf-env": "^1.1.2",
    "gulp": "^3.9.1",
    "gulp-cucumber": "0.0.23",
    "gulp-develop-server": "^0.5.2",
    "gulp-watch": "^5.0.1",
    "npm-run-all": "^4.1.3",
    "polyserve": "^0.27.13",
    "standard-version": "^4.4.0"
  }
}
